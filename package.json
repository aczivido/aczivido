    "upload": "rsync -avSz --delete-after dist/"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "gulp": "^3.9.1",
    "gulp-imagemin": "^3.2.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-sequence": "^0.4.6",
    "gulp-size": "^2.1.0"
  }
}
